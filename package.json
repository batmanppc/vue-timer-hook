{
  "name": "vue-timer-hook",
  "version": "0.0.6",
  "description": "Vue timer hook is a vue 3 hook, built to handle timer, stopwatch, and time logic/state in your vue component.",
  "author": "Martin Donadieu <martindonadieu@gmail.com>",
  "repository": "https://github.com/riderx/vue-timer-hook",
  "license": "MIT",
  "keywords": [
    "typescript",
    "vue",
    "hook"
  ],
  "main": "./dist/tsc/index.js",
  "types": "./dist/tsc/index.d.ts",
  "browser": "./dist/esbuild/index.js",
  "scripts": {
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/index.ts",
    "build": "tsc -p tsconfig.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "release": "bash scripts/release.sh",
    "build-all": "yarn clean && yarn build && yarn esbuild",
    "esbuild": "esbuild src/index.ts --bundle --minify --sourcemap=external --outfile=dist/esbuild/index.js",
    "esbuild:dev": "esbuild src/index.ts --bundle --outfile=dist/esbuild/index.js",
    "esbuild:watch": "esbuild src/index.ts --bundle --watch --outfile=dist/esbuild/index.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "conventional-changelog-cli": "^2.1.1",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "vue": "^3.2.1"
  }
}
